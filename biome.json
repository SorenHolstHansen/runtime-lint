{
	"$schema": "https://biomejs.dev/schemas/1.8.1/schema.json",
	"files": {
		"ignore": ["**/dist/*"]
	},
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	}
}
