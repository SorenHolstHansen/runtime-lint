{
	"name": "network-lint",
	"version": "0.0.1",
	"description": "A runtime linter for your network calls.",
	"scripts": {
		"test": "node --import tsx --test test/**/*.ts",
		"build": "tsc"
	},
	"dependencies": {
		"object-stats": "*"
	},
	"devDependencies": {
		"@types/node": "^20.14.9",
		"tsx": "^4.15.7"
	},
	"keywords": ["fetch", "network"],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"license": "MIT"
}
